{
   "regexManagers": [
      {
         "datasourceTemplate": "github-releases",
         "extractVersionTemplate": "^v(?<version>.*)$",
         "fileMatch": [
            ".*/\\.tflint\\.hcl$",
            "^\\.tflint\\.hcl$"
         ],
         "matchStrings": [
            "\"(?<currentValue>[^ '\"]+?)\" +# renovate: depName=(?<depName>.*?)\\n",
            "'(?<currentValue>[^ '\"]+?)' +# renovate: depName=(?<depName>.*?)\\n"
         ]
      }
   ]
}
