{
  "regexManagers": [
    {
      "fileMatch": [
        "^renovate\\.json5?$",
        "^\\.github/renovate\\.json5?$",
        "^\\.gitlab/renovate\\.json5?$",
        "^\\.renovaterc\\.json$",
        "^\\.renovaterc$"
      ],
      "matchStrings": [
        "\"github>{{arg0}}#(?<currentValue>[^\" \\n\\(]+)",
        "\"github>{{arg0}}:.*#(?<currentValue>[^\" \\n\\(]+)",
        "\"github>{{arg0}}/.*#(?<currentValue>[^\" \\n\\(]+)"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "{{arg0}}"
    }
  ]
}
